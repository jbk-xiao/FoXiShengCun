tStatistic/844099\")\r\n                let saleInfo = await axios.get(\"http://121.46.19.26:8511/getPredict/\" + \"老干妈\") //销量+预测数据\r\n                let qpInfo = await axios.get(\"http://121.46.19.26:8511/getRelateSearch/\" + \"老干妈\") //关联搜索气泡图数据\r\n                let siInfo = await axios.get(\"http://121.46.19.26:8511/getIndexPredict/\" + \"老干妈\") //搜索指数数据\r\n                let ageInfo = await axios.get(\"http://121.46.19.26:8511/getAgeDistribution/\" + \"老干妈\") //搜索指数数据\r\n                let regInfo = await axios.get(\"http://121.46.19.26:8511/getProvinceIndex/\" + \"老干妈\") //地区搜索指数数据\r\n                let newsInfo = await axios.get(\"http://121.46.19.26:8511/getNews/\" + \"老干妈\") //新闻数据\r\n                \r\n                let treeInfo = await axios.get(\"http://121.46.19.26:8511/getEmotionAnalysis/\"+\"老干妈\") //树图数据                \r\n                let {\r\n                    data\r\n                } = await axios.get(\r\n                    \"http://121.46.19.26:8511/getGraphByBrand/\" +\r\n                    \"陶华碧老干妈\");\r\n                this.tinfo = threeInfo.data\r\n                this.info = res.data.scoreList\r\n                this.cinfo = res.data.companyInfo \r\n                this.sales = saleInfo.data\r\n                this.treeinfo = treeInfo.data\r\n                this.commentInfo = commentInfo\r\n                this.qinfo = qpInfo.data\r\n                this.iinfo = siInfo.data\r\n                this.pinfo = ageInfo.data\r\n                this.dinfo = regInfo.data\r\n                this.news = newsInfo.data\r\n                                \r\n                this.rinfo = data;\r\n                console.log(this.tinfo)\r\n                /*console.log(this.iinfo)\r\n                console.log(this.sales)\r\n                                                \r\n                console.log(res.data.scoreList)\r\n                console.log(this.cinfo)\r\n                console.log(this.info)\r\n                console.log(this.rinfo)\r\n                console.log(this.tinfo)*/\r\n                this.keynum += 1\r\n                this.keyn+=1\r\n                this.keyd+=1\r\n                //console.log(this.commentInfo)\r\n            },\r\n            async getInfo1(item) {\r\n               \r\n                let qpInfo = await axios.get(\"http://121.46.19.26:8511/getRelateSearch/\" + item) //关联搜索气泡图数据\r\n                let siInfo = await axios.get(\"http://121.46.19.26:8511/getIndexPredict/\" + item) //搜索指数数据\r\n                let ageInfo = await axios.get(\"http://121.46.19.26:8511/getAgeDistribution/\" + item) //搜索指数数据\r\n                let regInfo = await axios.get(\"http://121.46.19.26:8511/getProvinceIndex/\" + item) //地区搜索指数数据\r\n                this.qinfo = qpInfo.data\r\n                this.iinfo = siInfo.data\r\n                this.pinfo = ageInfo.data\r\n                this.dinfo = regInfo.data\r\n                //console.log(this.qinfo)\r\n                this.keynum += 1\r\n            }\r\n        },\r\n        created() {\r\n            console.log('mounted()');\r\n            //setTimeout(() => {\r\n            this.getInfo();\r\n            //}, 500);\r\n            this.loading();\r\n        },\r\n        mounted() {\r\n    this.$nextTick(() => {\r\n      this.topMenu();\r\n    });\r\n  },\r\n  activated() {\r\n    this.$nextTick(() => {\r\n      var menu = this.$refs.menu;\r\n      if (menu.getBoundingClientRect().top <= 0) {\r\n        this.isTop = true;\r\n        this.placeholder = true;\r\n      } else {\r\n        this.isTop = false;\r\n        this.placeholder = false;\r\n      }\r\n    });\r\n  },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .blog-details__image img {\r\n        width: 100%;\r\n    }\r\n\r\n    .block-title {\r\n        width: 100;\r\n        margin: 0 auto;\r\n        text-align: center;\r\n    }\r\n\r\n    .el-row {\r\n        width: 100%;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .list {\r\n        width: 100;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .project-details__list {\r\n        width: 90%;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .project-details__list li {\r\n        width: 100%;\r\n        margin: 0 auto;\r\n    }\r\n    .project-details__list li span{\r\n        width: 100%;\r\n        margin: 0 auto;\r\n    }\r\n    .project-details__list li span a {\r\n        color: var(--thm-black);\r\n        \r\n    }\r\n\r\n    .block-title__image {\r\n        left: 30%;\r\n        width: 24px;\r\n        margin: 0 auto;\r\n        margin-bottom: 5px;\r\n    }\r\n\r\n    .project-details {\r\n        padding-bottom: 20px;\r\n        padding-top: 30px;\r\n    }\r\n    .list-unstyled.project-details__list{\r\n        width: 90%;\r\n    }\r\n    #namelist {\r\n        height: 50px;\r\n        width: 550px;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    #namelist ul {\r\n        width: 85px;\r\n        list-style: none;\r\n\r\n        height: 44px;\r\n        margin: 0 auto;\r\n        display: inline;\r\n    }\r\n\r\n    #namelist li {\r\n        width: 85px;\r\n        margin: 0 auto;\r\n        display: inline;\r\n        line-height: 40px;\r\n        float: left;\r\n        margin-left: 5px;\r\n        border-radius: 3px;\r\n    }\r\n\r\n    #namelist button {\r\n\r\n        width: 85px;\r\n        background-color: #f7c35f;\r\n        display: block;\r\n        margin: 0 auto;\r\n        \r\n    }\r\n\r\n    .block-title h3 {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    #guide {\r\n        width: 190px;\r\n        margin: 0 auto;\r\n        font-size: 30px;\r\n        font-weight: bold;\r\n        color: rgb(37, 89, 70);\r\n    }\r\n    #top {\r\n        width: 80%;\r\n        margin: 0 auto;\r\n    }\r\n</style>\r\n<style lang=\"scss\">\r\n    .create-isLoading .el-loading-text {\r\n        color: #f7c35f;\r\n        font-size: 18px;\r\n    }\r\n    .create-isLoading i {\r\n        color: #f7c35f;\r\n    }\r\n</style>\r\n<style scoped>\r\n.menu {\r\n 