orage.getItem(key);\r\n\tif( typeof str == \"string\" ){\r\n\t\ttry{\r\n\t\t\treturn JSON.parse(str);\r\n\t\t}catch(e) {\r\n\t\t\treturn str;\r\n\t\t}\r\n\t}\r\n\treturn;\r\n},\r\n\r\n    currentChange(val) {\r\n      this.page = val;\r\n      //alert(val);\r\n      this.setContextData(\"currentPage\",this.page)\r\n      this.$parent.point(val);\r\n    },\r\n    prevpage(val) {\r\n      this.page = val;\r\n      //console.log(val);\r\n      this.$parent.point(val);\r\n    },\r\n    nextpage(val) {\r\n      this.page = val;\r\n      // console.log(val);\r\n      this.$parent.point(val);\r\n    },\r\n  },\r\n\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/components"}]}