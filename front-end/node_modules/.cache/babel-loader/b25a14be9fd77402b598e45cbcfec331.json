A;AADA,WAPA;AAYA,UAAA,IAAA,EAAA,CAAA;AACA,YAAA,KAAA,EAAA,CACA,IAAA,CAAA,CAAA,CAAA,CAAA,aAAA,GAAA,EADA,EAEA,IAAA,CAAA,CAAA,CAAA,CAAA,WAAA,GAAA,EAFA,EAGA,IAAA,CAAA,CAAA,CAAA,CAAA,eAAA,GAAA,EAHA,EAIA,IAAA,CAAA,CAAA,CAAA,CAAA,WAAA,GAAA,EAJA,EAKA,IAAA,CAAA,CAAA,CAAA,CAAA,aAAA,GAAA,EALA,CADA;AAQA,YAAA,IAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AARA,WAAA;AAZA,SAAA,EAHA,CA0BA;AACA,OAlCA,CAoCA;;;AACA,MAAA,OAAA,CAAA,SAAA,CAAA;AACA,QAAA,OAAA,EAAA;AACA,UAAA,OAAA,EAAA;AADA,SADA;AAIA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA,QADA;AAEA,UAAA,MAAA,EAAA,EAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAJA;AASA,QAAA,SAAA,EAAA;AACA,UAAA,GAAA,EAAA,QADA;AAEA,UAAA,KAAA,EAAA,EAFA;AAGA,UAAA,KAAA,EAAA,CAAA,KAAA,EAAA,QAAA,CAHA;AAIA,UAAA,UAAA,EAAA,IAJA;AAKA,UAAA,GAAA,EAAA,CALA;AAMA,UAAA,GAAA,EAAA;AANA,SATA;AAiBA,QAAA,KAAA,EAAA;AACA,UAAA,SAAA,EAAA,CAAA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,GAAA,EAAA;AAFA,WAAA,EAIA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,GAAA,EAAA;AAFA,WAJA,EAQA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,GAAA,EAAA;AAFA,WARA,EAYA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,GAAA,EAAA;AAFA,WAZA,EAgBA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,GAAA,EAAA;AAFA,WAhBA;AADA,SAjBA;AAwCA,QAAA,MAAA,EAAA;AAxCA,OAAA;AA0CA,MAAA,MAAA,CAAA,QAAA,GAAA,OAAA,CAAA,MAAA;AACA;AAjFA;AAbA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <section class=\"project-details\">\r\n            \r\n                <div class=\"block-title\">\r\n                    <p>Product Comment Dimension Radar</p>\r\n                    <h3>竞品评价维度雷达图</h3>\r\n                </div>\r\n\r\n            <div class=\"chart1\" id=\"main\" ref=\"chart\"></div>\r\n\r\n        </section><!-- /.project-details -->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'eCharts',\r\n        props: ['info'],\r\n        data() {\r\n            return {\r\n                list: []\r\n            }\r\n        },\r\n        mounted() {\r\n            setTimeout(() => {\r\n                this.draw(this.info)\r\n            }, 1000);\r\n        },\r\n        methods: {\r\n            draw(info) {\r\n                //console.log(info)\r\n                var myChart = this.$echarts.init(document.getElementById('main'))\r\n                \r\n                var list = [];\r\n                var series = [];\r\n\r\n                for(let i = 0; i < info.length; i++){\r\n                // info.forEach(function (e) {\r\n                    list.push(info[i].brand);\r\n                    series.push({\r\n                        name: '评价维度',\r\n                        type: 'radar',\r\n                        symbol: 'none',\r\n                        lineStyle: {\r\n                            width: 1\r\n                        },\r\n                        emphasis: {\r\n                            areaStyle: {\r\n                                color: 'rgba(0,250,0,0.3)'\r\n                            }\r\n                        },\r\n                        data: [{\r\n                            value: [\r\n                                info[i].package_score * 10,\r\n                                info[i].price_score*10,\r\n                                info[i].logistics_score*10,\r\n                                info[i].taste_score*10,\r\n                                info[i].service_score*10,\r\n                            ],\r\n                            name: info[i].brand\r\n                        }]\r\n                    })\r\n                // });\r\n                }\r\n\r\n                // 绘制图表\r\n                myChart.setOption({\r\n                    tooltip: {\r\n                        trigger: 'item'\r\n                    },\r\n                    legend: {\r\n                        type: 'scroll',\r\n                        bottom: 10,\r\n                        data: list\r\n                    },\r\n                    visualMap: {\r\n                        top: 'middle',\r\n                        right: 10,\r\n                        color: ['red', 'yellow'],\r\n                        calculable: true,\r\n                        min: 0,\r\n                        max: 10\r\n                    },\r\n                    radar: {\r\n                        indicator: [{\r\n                                text: '包装',\r\n                                max: 10\r\n                            },\r\n                            {\r\n                                text: '价格',\r\n                                max: 10\r\n                            },\r\n                            {\r\n                                text: '物流',\r\n                                max: 10\r\n                            },\r\n                            {\r\n                                text: '味道',\r\n                                max: 10\r\n                            },\r\n                            {\r\n                                text: '服务',\r\n                                max: 10\r\n                            }\r\n                        ]\r\n                    },\r\n                    series: series\r\n                });\r\n                window.onresize = myChart.resize;\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .chart1 {\r\n        width: 100%;\r\n        height: 400px;\r\n        margin: 50px auto;\r\n        border: 1px solid rgb(246, 245, 242);\r\n        box-shadow: 8px 8px 8px rgba(0, 0, 0, .2)\r\n    }\r\n    .block-title{\r\n    width: 100;\r\n    margin: 0 auto;\r\n    text-align:center;\r\n  }\r\n  .project-details{\r\n      padding-bottom: 20px;\r\n      padding-top: 30px;\r\n  }\r\n  .block-title h3{\r\n        margin-bottom: 20px;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}